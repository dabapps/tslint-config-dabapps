{
  "name": "tslint-config-dabapps",
  "version": "0.4.0",
  "description": "DabApps ESLint Configuration",
  "main": "tslint.json",
  "dependencies": {
    "tslint-eslint-rules": "4.0.0",
    "tslint-react": "3.2.0"
  },
  "devDependencies": {
    "@types/react": "16.0.19",
    "@types/react-dom": "16.0.2",
    "jsonlint": "1.6.2",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "tslint": "5.8.0",
    "typescript": "2.3.3"
  },
  "peerDependencies": {
    "typescript": ">=2.3.3 <3.x.x",
    "tslint": ">=5.8.x <6.x.x"
  },
  "scripts": {
    "jsonlint": "find . -path ./node_modules -prune -o -type f -name '*.json' -exec jsonlint -q '{}' \\;",
    "tslint": "tslint --project 'ts/tsconfig.json' 'ts/**/*.{ts,tsx}'",
    "test": "npm run jsonlint && npm run tslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dabapps/tslint-config-dabapps.git"
  },
  "keywords": [
    "tslint",
    "linting",
    "typescript"
  ],
  "author": "DabApps",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/dabapps/tslint-config-dabapps/issues"
  },
  "homepage": "https://github.com/dabapps/tslint-config-dabapps#readme"
}
